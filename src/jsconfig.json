{
    "compilerOptions": {
        "target": "es6",
        "baseUrl": "src",
        "jsx": "react",
        "paths": {
            "components:/*":[
                "./components/*"
            ],
            "assets:/*":[
                "./assets/*"
            ],
            "containers:/*":[
                "./containers/*"
            ],
            "commons:/*":[
                "./commons/*"
            ],
            "resources/*": [
                "./resources/*"
            ],
        }
    },
    "exclude": [
      "node_modules",
      "build"
    ]
}